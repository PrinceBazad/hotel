{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(booking|dashboard|login|logout|setup_database)\\.php",
      "dest": "/api/$1.php"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
