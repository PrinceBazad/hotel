{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.php",
      "use": "vercel-php@0.6.0"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(booking|dashboard|login|logout|setup_database)\\.php",
      "dest": "/api/$1.php"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
